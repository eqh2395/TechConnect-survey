   "cell_type": "code",
   "execution_count": 104,
   "metadata": {},
   "outputs": [],
   "source": [
    "def pull_tools(df, answer_list):\n",
    "    answer_count = dict()\n",
    "    for word in answer_list:\n",
    "        for index, row in df.iterrows():\n",
    "            if word in row:\n",
    "                if word in answer_count.keys():\n",
    "                    answer_count[word] = answer_count[word] + 1\n",
    "                else:\n",
    "                    answer_count[word] = 1\n",
    "    return answer_count\n",
    "                \n",
    "tools = pull_tools(visualisation_df, languages)"
   ]
